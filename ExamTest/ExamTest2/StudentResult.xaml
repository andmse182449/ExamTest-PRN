<Window x:Class="ExamTest_PRN212.StudentResult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExamTest_PRN212"
        mc:Ignorable="d"
        Title="StudentResult" Height="200" Width="500" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Name="ResultText" FontSize="20" TextAlignment="Center" Margin="10"/>
            <Button Name="CloseButton" Content="Close" Width="100" Height="30" Click="CloseButton_Click" HorizontalAlignment="Center"/>
         
        </StackPanel>
    </Grid>
</Window>
